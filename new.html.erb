<h1>Users#new</h1>
<p>find me in app/views/users/new/html.erb></p>
<%= form_for @user do |u| %>
  <%= u.label :email %>
  <%= u.text_field :email%>
  <%= u.label :password %>
  <%= u.password_field :password %>
  <%= u.label :password_confirmation %>
  <%= u.password_field :password_confirmation %>
  <%= u.submit "sign up" %>
  <% end %>
</form>
